package Task1;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.ResultSet;
import java.sql.Statement;



public class Reading {

	public static void main(String[] args) {
		
		try{  
			Class.forName("com.mysql.jdbc.Driver");  
			Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/techouts","root","root");  
			Statement s=con.createStatement(); 
			
			ResultSet r=s.executeQuery("select * from studdetails where id = 1 ");  
			while(r.next())  
				
			System.out.println(r.getInt(1)+"  "+r.getString(2)+"  "+r.getString(3) +" "+r.getString(4));
			
			con.close(); 
			
			}
		
		catch(Exception e)
		
		{
			System.out.println(e);
			
		}  
	}

}
